#include<stdio.h>

void Check_char(char x);

int main(void)
{
	char y;
	printf("Enter a character: ");
	scanf("%c", &y);
	Check_char(y);
}

void Check_char(char x)
{
	if ((x >= 'a' && x <= 'z') || (x >= 'A' && x <= 'Z'))
		printf("%c is an alphabet", x);
	else
		printf("%c is not an alphabet", x);
}
