//program to check whether a number is Odd or Even
//Eng: Amr Ahmed

#include <stdio.h>

void check_nu_type(int x);

int main(void)
   {

	fflush(stdin);
	fflush(stdout);
   }
void check_nu_type(int x)
    {
	 if(x%2 == 1)
	 {
		 printf("%d is even", x);
	 }
	 else if (x%2 != 1)
	 {
		 printf("%d is odd", x);
	 }
	 else
	 {
		 printf("please try again with right value");
	 }
	}

