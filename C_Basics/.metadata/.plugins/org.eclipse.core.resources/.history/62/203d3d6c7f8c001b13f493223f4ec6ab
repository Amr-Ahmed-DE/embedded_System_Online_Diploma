#include <stdio.h>

int main(void)
{
	float x, y, z;
	printf("Enter three numbers: ");
	fflush(stdin);
	fflush(stdout);
	scanf("%f %f %f", &x, &y, &z);

	if(x > y)
	{
		if(y > z)
		{
			printf("%f largest number", x);
		}else if(z > x)
		{
			printf("%f largest number", z);
		}
	}else if (y > z)
	{
		printf("%f largest number", y);
	}
}
